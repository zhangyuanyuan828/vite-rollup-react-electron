{
  "name": "vite-rollup-react-electron",
  "description": "vite rollup react electron demo",
  "author": "玲玲子 <zhangyuanyuan828@outlook.com>",
  "private": true,
  "version": "0.0.1",
  "main": "dist/main/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development ts-node scripts/start.ts",
    "build": "cross-env NODE_ENV=production tsc && vite build && rollup -c rollup.config.ts && electron-builder --config electron-builder.config.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "postinstall": "electron-builder install-app-deps",
    "prepare": "husky install"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-eslint": "^8.0.2",
    "@rollup/plugin-node-resolve": "^13.2.1",
    "@rollup/plugin-replace": "^4.0.0",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@vitejs/plugin-react": "^1.3.1",
    "cross-env": "^7.0.3",
    "electron": "^18.0.4",
    "electron-builder": "^23.0.3",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.13.0",
    "eslint-config-react-app": "^7.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.8",
    "rollup": "^2.70.2",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.5",
    "vite-plugin-eslint": "^1.4.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-template-curly-in-string": "off"
    }
  },
  "browserslist": [
    "last 1 electron version"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --fix"
  }
}
